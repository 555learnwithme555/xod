    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.11.6",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "dirty-chai": "^1.2.2",
    "eslint": "^2.13.0",
    "eslint-plugin-import": "^1.8.1",
    "espruino": "git+https://github.com/espruino/EspruinoTools.git#gh-pages",
    "json5": "<0.5.0",
