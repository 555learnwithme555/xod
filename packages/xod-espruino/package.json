{
  "name": "xod-espruino",
  "version": "0.0.1",
  "description": "XOD project: Espruino runtime",
  "scripts": {
    "dev": "webpack --watch --colors --config webpack/base.js",
    "build": "mkdir -p dist && webpack --colors --config webpack/base.js",
    "lint:espruino": "eslint --no-eslintrc --config eslintrc.espruino.js ./src/runtime.js ./nodes/js ./nodes/espruino || true",
    "lint:es6": "eslint ./src --ignore-pattern runtime.js || true",
    "lint": "npm-run-all lint:*",
    "test": "mocha-webpack \"test/**/*.spec.js\""
  },
  "repository": {},
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "co": "^4.6.0",
    "espruino": "git+https://github.com/espruino/EspruinoTools.git#gh-pages",
    "jquery": "^3.1.0",
    "ramda": "^0.21.0"
  },
  "engines": {
    "node": "6.0.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.6.1",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-import-resolver-webpack": "^0.3.2",
    "mocha": "^2.3.4",
    "mocha-webpack": "^0.4.0",
    "npm-run-all": "^2.3.0",
    "source-map-support": "^0.4.2"
  },
  "peerDependencies": {
    "xod-core": "^0.0.1"
  }
}
