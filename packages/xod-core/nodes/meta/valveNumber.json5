{
  label: 'valveNumber',
  inputs: [{
    key: 'cond',
    type: 'bool',
    description: 'Condition'
  },
  {
    key: 'in',
    type: 'number',
    description: 'Input'
  }],
  outputs: [{
    key: 'out',
    type: 'number',
    description: 'Output (receives messages from the input if condition is "true")',
  }],
  impl: {
    js: "\nmodule.exports.evaluate = function(e) {\n  if (e.inputs.cond) {\n    e.fire({ out: e.inputs.in });\n  }\n};\n",
  },
}
