{
  "name": "project",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "required": false,
      "defaultFn": "now"
    },
    "updatedAt": {
      "type": "date",
      "required": false,
      "defaultFn": "now"
    },
    "pojo": {
      "type": "string"
    },
    "public": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "author": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    }
  },
  "methods": {}
}
