{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "strict": false,
  "properties": {
    "username": {
      "type": "string",
      "required": true
    },
    "userpic": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "projects": {
      "type": "hasMany",
      "model": "project",
      "foreignKey": "ownerId"
    },
    "packages": {
      "type": "hasMany",
      "model": "package",
      "foreignKey": "ownerId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
